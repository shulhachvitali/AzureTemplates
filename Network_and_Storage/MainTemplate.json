{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
		"Network": {
		    "type": "array",
			"defaultValue": []
		},
		"storageAccount": {
		    "type": "array",
			"defaultValue": []
		},
		"Templates": {
		    "type": "array",
			"defaultValue": []
		},
	},
	"variables": {
		"ResourcesLocation": "[resourceGroup().location]",
		"Params": "https://raw.githubusercontent.com/shulhachvitali/AzureTemplates/master/Network_and_Storage/PARAMETERS.json",
		"StorageAccountL":"https://raw.githubusercontent.com/shulhachvitali/AzureTemplates/master/Network_and_Storage/StorageTemplate.json",
		"NetworkL":"https://raw.githubusercontent.com/shulhachvitali/AzureTemplates/master/Network_and_Storage/Network.json"
    },
	"resources": [
		{
			"apiVersion": "2017-05-10",
			"name": "StorageAccount",
			"type": "Microsoft.Resources/deployments",
			"properties": {
			"mode": "incremental",
			"templateLink": {
				"uri":"[variables('StorageAccountL')]",
				"contentVersion":"1.0.0.0"
			},
			"parametersLink": {
			"uri": "[variables('Params')]",
			"contentVersion": "1.0.0.0"
			},
			},
		
		},
		{
			"apiVersion": "2017-05-10",
			"name": "Network",
			"type": "Microsoft.Resources/deployments",
			"properties": {
			"mode": "incremental",
			"templateLink": {
				"uri":"[variables('NetworkL')]",
				"contentVersion":"1.0.0.0"
			},
			"parametersLink": {
			"uri": "[variables('Params')]",
			"contentVersion": "1.0.0.0"
			},
			},
		
		},
		
	]
}
	